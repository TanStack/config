{
  "name": "@tanstack/config",
  "version": "0.17.0",
  "description": "All-in-one config used by TanStack projects.",
  "author": "tannerlinsley",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TanStack/config.git",
    "directory": "packages/config"
  },
  "homepage": "https://tanstack.com/config",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/tannerlinsley"
  },
  "scripts": {
    "test:types": "tsc",
    "test:eslint": "eslint ./src",
    "test:build": "publint --strict"
  },
  "type": "module",
  "exports": {
    "./eslint": {
      "import": {
        "default": "./src/eslint/index.js"
      }
    },
    "./publish": {
      "import": {
        "types": "./src/publish/index.d.ts",
        "default": "./src/publish/index.js"
      }
    },
    "./typedoc": {
      "import": {
        "types": "./src/typedoc/index.d.ts",
        "default": "./src/typedoc/index.js"
      }
    },
    "./vite": {
      "import": {
        "types": "./src/vite/index.d.ts",
        "default": "./src/vite/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "src"
  ],
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@tanstack/eslint-config": "workspace:*",
    "@tanstack/publish-config": "workspace:*",
    "@tanstack/typedoc-config": "workspace:*",
    "@tanstack/vite-config": "workspace:*"
  },
  "devDependencies": {
    "eslint": "catalog:"
  }
}
