{
  "name": "@tanstack/config",
  "version": "0.14.2",
  "description": "Configuration and tools for publishing and maintaining TanStack projects.",
  "author": "tannerlinsley",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TanStack/config.git",
    "directory": "packages/config"
  },
  "homepage": "https://tanstack.com/config",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/tannerlinsley"
  },
  "scripts": {
    "test:types": "tsc",
    "test:eslint": "eslint ./bin ./src",
    "test:build": "publint --strict"
  },
  "type": "module",
  "exports": {
    "./eslint": {
      "import": {
        "default": "./src/eslint/index.js"
      }
    },
    "./publish": {
      "import": {
        "types": "./src/publish/index.d.ts",
        "default": "./src/publish/index.js"
      }
    },
    "./typedoc": {
      "import": {
        "types": "./src/typedoc/index.d.ts",
        "default": "./src/typedoc/index.js"
      }
    },
    "./vite": {
      "import": {
        "types": "./src/vite/index.d.ts",
        "default": "./src/vite/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "tanstack-config": "./bin/config.js"
  },
  "preferGlobal": false,
  "sideEffects": false,
  "files": [
    "bin",
    "src"
  ],
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@commitlint/parse": "^19.5.0",
    "@eslint/js": "^9.16.0",
    "@stylistic/eslint-plugin-js": "^2.12.0",
    "commander": "^12.1.0",
    "current-git-branch": "^1.1.0",
    "esbuild-register": "^3.6.0",
    "eslint-plugin-import-x": "^4.5.0",
    "eslint-plugin-n": "^17.14.0",
    "globals": "^15.13.0",
    "interpret": "^3.1.1",
    "jsonfile": "^6.1.0",
    "liftoff": "^5.0.0",
    "minimist": "^1.2.8",
    "rollup-plugin-preserve-directives": "^0.4.0",
    "semver": "^7.6.3",
    "simple-git": "^3.27.0",
    "typedoc": "^0.27.4",
    "typedoc-plugin-frontmatter": "^1.1.0",
    "typedoc-plugin-markdown": "^4.3.2",
    "typescript-eslint": "^8.17.0",
    "v8flags": "^4.0.1",
    "vite-plugin-dts": "4.0.3",
    "vite-plugin-externalize-deps": "^0.8.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "devDependencies": {
    "@types/current-git-branch": "^1.1.6",
    "@types/eslint": "^9.6.1",
    "@types/interpret": "^1.1.3",
    "@types/jsonfile": "^6.1.4",
    "@types/liftoff": "^4.0.3",
    "@types/minimist": "^1.2.5",
    "@types/semver": "^7.5.8",
    "@types/v8flags": "^3.1.3",
    "eslint": "^9.16.0",
    "type-fest": "^4.30.0"
  }
}
