{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "defaultBase": "main",
  "nxCloudId": "65cc80a2ddc7c22ebb07831c",
  "useInferencePlugins": false,
  "parallel": 5,
  "tui": {
    "enabled": false
  },
  "targetDefaults": {
    "test:docs": {
      "cache": true,
      "inputs": ["{workspaceRoot}/docs/**/*"]
    },
    "test:sherif": {
      "cache": true,
      "inputs": ["{workspaceRoot}/**/package.json"]
    },
    "test:eslint": {
      "cache": true
    },
    "test:types": {
      "cache": true
    },
    "build": {
      "cache": true
    },
    "test:build": {
      "cache": true,
      "dependsOn": ["build"]
    }
  }
}
